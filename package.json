{
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build && yarn build-tailwind",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "yarn build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build-tailwind": "NODE_ENV=production npx tailwindcss -o ./dist/tailwind.css --minify"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "awesome-component-library",
  "author": "Chris Scott",
  "module": "dist/awesome-component-library.esm.js",
  "size-limit": [
    {
      "path": "dist/awesome-component-library.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/awesome-component-library.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@size-limit/preset-small-lib": "^5.0.1",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@tailwindcss/postcss7-compat": "^2.2.4",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.3.1",
    "babel-loader": "^8.2.2",
    "husky": "^7.0.1",
    "postcss": "^8.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^5.0.1",
    "tailwindcss": "^2.2.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@headlessui/react": "^1.3.0"
  }
}
